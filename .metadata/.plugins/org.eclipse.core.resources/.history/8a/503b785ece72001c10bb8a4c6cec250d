package q2;

public class CharArray {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		char[] alphabets = new char[26];
		char ch = 'A';
		
		for(int i = 0; i < alphabets.length; i++) {
			alphabets[i] = ch;
			ch++;
			System.out.println(ch);
		}
		
		//for(int i = 0; i < alphabets.length; i++) {
//	}

}
